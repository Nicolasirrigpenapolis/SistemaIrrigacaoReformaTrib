Private Sub Duplicar()
   Dim MovimentoOriginal As New GRecordSet, MProdutosOriginal As New GRecordSet
   Dim MovimentoCopia As New GRecordSet, MProdutosCopia As New GRecordSet
   Dim LicAux As Long
         
   On Error GoTo DeuErro
   
   If MsgBox("Tem Certeza que Deseja Duplicar esse Pre Pedido?" & vbCrLf & "", vbExclamation + vbYesNo, vaTitulo) = vbNo Then Exit Sub
   
   Screen.MousePointer = vbHourglass 'Ampulheta
   vgDb.BeginTrans
      
   Set MovimentoOriginal = vgDb.OpenRecordSet("SELECT * FROM [Pedido de Compra Novo] " & _
                                              "WHERE [Id do Pedido] = " & seqMvto)
   Set MovimentoCopia = vgDb.OpenRecordSet("SELECT * FROM [Pedido de Compra Novo]")
   Set MProdutosOriginal = vgDb.OpenRecordSet("SELECT * FROM [Produtos do Pedido Compra] " & _
                                              "WHERE [Id do Pedido] = " & seqMvto)
   Set MProdutosCopia = vgDb.OpenRecordSet("SELECT * FROM [Produtos do Pedido Compra]")
   
   
   With MovimentoOriginal
      MovimentoCopia.AddNew
      MovimentoCopia![Data Do Pedido] = Date
      MovimentoCopia![Codigo Do Fornecedor] = ![Codigo Do Fornecedor]
      MovimentoCopia![Codigo da Transportadora] = ![Codigo da Transportadora]
      MovimentoCopia!Comprador = vgPWUsuario
      MovimentoCopia!Prazo = !Prazo
      MovimentoCopia![Total dos Produtos] = ![Total dos Produtos]
      MovimentoCopia![Total Do Pedido] = ![Total Do Pedido]
      MovimentoCopia![Seqüência Grupo Despesa] = ![Seqüência Grupo Despesa]
      MovimentoCopia![Seqüência SubGrupo Despesa] = ![Seqüência SubGrupo Despesa] 
      MovimentoCopia!PrePedido = 1
      MovimentoCopia!Dias = !Dias
      MovimentoCopia![Nome Do Banco 1] = " "
      MovimentoCopia![Agência Do Banco 1] = " "
      MovimentoCopia![Conta Corrente Do Banco 1] = " "
      MovimentoCopia![Nome Do Correntista Do Banco 1] = " "
      MovimentoCopia!CifFob = !CifFob
      MovimentoCopia![Previsao de Entrega] = Date
      MovimentoCopia.Update
       LicAux = MovimentoCopia![Id Do Pedido]
      MovimentoCopia.BookMark = MovimentoCopia.LastModified
      'Produtos
      If MProdutosOriginal.RecordCount > 0 Then
         Do While Not MProdutosOriginal.EOF
            MProdutosCopia.AddNew
            MProdutosCopia![Id Do Pedido] = LicAux 
            MProdutosCopia![Id Do Produto] = MProdutosOriginal![Id Do Produto]
            MProdutosCopia![Sequencia Do Item] = MProdutosOriginal![Sequencia Do Item]
            MProdutosCopia!Qtde = MProdutosOriginal!Qtde
            MProdutosCopia![Vr Unitario] = MProdutosOriginal![Vr Unitario]
            MProdutosCopia![Vr Total] = MProdutosOriginal![Vr Total]
            MProdutosCopia![Aliquota Do IPI] = MProdutosOriginal![Aliquota Do IPI]
            MProdutosCopia![Vr Do IPI] = MProdutosOriginal![Vr Do IPI]
            MProdutosCopia![Aliquota Do ICMS] = MProdutosOriginal![Aliquota Do ICMS]
            MProdutosCopia![Vr Do ICMS] = MProdutosOriginal![Vr Do ICMS]
            MProdutosCopia.Update
            MProdutosCopia.BookMark = MProdutosCopia.LastModified            
            MProdutosOriginal.MoveNext
         Loop
      End If
   
   End With
        
   vgDb.CommitTrans 'Comitando
   
   MsgBox "Pre Pedido Duplicado com Sucesso!!!", vbInformation + vbOKOnly, vaTitulo      
   
   Unload Me
   
   PrePedido.Show
   PrePedidoAberto = True
   MoveRegistro PrePedido, REG_ULTIMO 
   
   Screen.MousePointer = vbDefault 'Ampulheta
   
DeuErro:
   If Err Then
      MsgBox Err.Description, vbCritical + vbOKOnly, vaTitulo
      vgDb.RollBackTrans
   End If
   
End Sub

