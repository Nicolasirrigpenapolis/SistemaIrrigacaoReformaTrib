'inicializa variaveis (apelidos) coms os campos correspondentes
Private Sub InicializaApelidos(vgComOQue As Integer)
   On Error Resume Next                           'prepara para possíveis erros
   If (vgTb.RecordCount > 0 And vgTb.Eof = False And vgTb.Bof = False) Or vgComOQue = COM_TEXTBOX Then
      If vgComOQue = COM_TEXTBOX Then
         Sequencia_do_Backup = txtCampo(0).Value
         Tipo_do_Backup = txtCampo(1).Value
         Hora = txtCampo(2).Value
         Segunda = chkCampo(0).Value
         Terca = chkCampo(1).Value
         Quarta = chkCampo(2).Value
         Quinta = chkCampo(3).Value
         Sexta = chkCampo(4).Value
         Sabado = chkCampo(5).Value
         Domingo = chkCampo(6).Value
         Dia = txtCampo(3).Value
         Destino = txtCampo(4).Value
      Else
         Sequencia_do_Backup = IIf(IsNull(vgTb![Seqüência do Backup]), 0, vgTb![Seqüência do Backup])
         Tipo_do_Backup = IIf(IsNull(vgTb![Tipo do Backup]), "", vgTb![Tipo do Backup])
         Hora = vgTb!Hora
         Segunda = IIf(IsNull(vgTb!Segunda), 0, vgTb!Segunda)
         Terca = IIf(IsNull(vgTb!Terça), 0, vgTb!Terça)
