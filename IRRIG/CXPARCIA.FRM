   vgSituacao = ACAO_NAVEGANDO
   Set botOk.Picture = LoadPicture(LoadGasPicture(37385))
   Set txtQtde_Caixinha = txtCampo(0)
   Set txtUnidadeFor = txtCampo(1)
   Set lblAjuste = Label(2)
   DefineControles
   vgTooltips.Create
   Label(0).Caption = LoadGasString(45885)
   vgTooltips.AddTool botOk, 0, LoadGasString(45886)
   botOk.Caption = LoadGasString(45887)
   Label(1).Caption = LoadGasString(45888)
   Label(2).Caption = LoadGasString(45889)
   AjustaTamanho Me
   Executar INICIALIZACOES
   InicializaApelidos COM_TEXTBOX
