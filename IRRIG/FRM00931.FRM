Private Sub ProssegueExecucao()
   On Error Resume Next
   Dim vgFGrid As New frmBrowse
   With vgFGrid
      Tag = "Entrada Contas"
      vgNovaQuery = "" & _
         "SELECT E.[Seqüência da Entrada] As Entrada, [Data de Entrada] As Emissão, Format(E.[Seqüência Do Geral], '000000') + ' - ' + G.[Razão Social] As [Razão Social], " & _
         "Case Conta WHEN 'P' THEN Documento Else Convert(Varchar, [Número da Duplicata], 120) END As [Doc./Dupl], [Valor Total], " & _
         "PC.[Número da Parcela] As PC, PC.[Data de Vencimento] As Vencimento, PC.[Valor da Parcela] " & _
         "FROM [Entrada Contas] E INNER JOIN Geral G ON E.[Seqüência do Geral] = G.[Seqüência do Geral] " & _
         "INNER JOIN [Parcelas Entrada Contas] PC ON E.[Seqüência da Entrada] = PC.[Seqüência da Entrada] " & _
         "WHERE " & Filtro()         
     ' NAO PODE TER ORDER BY
      .vgTitConsulta = Tag
      With .grdBrowse
         .DefineOrderColumn .Columns("Seqüência").Index, -1
         '.ColumnWidth("Código do Produto") = 500
         '.ColumnWidth("Descrição do Produto") = 2000
         .RemoveInitialTop = True
        '.FullRowSelect = True
         .ShowFilterBar = True
         .Stripes = True
         '.Locked = True
      End With
      .Show
      DoEvents
      .grdBrowse.SetFocusFilter
    End With
SaiDaSub:
    Unload Me
End Sub

Private Function Filtro() As String
   Filtro = "1 = 1"
   If Entrada > 0 Then 'Pesquisou pela Seqüência da Entrada
      Filtro = Filtro & " AND E.[Seqüência da Entrada] = " & Entrada
   Else
      If Tipo = 0 Then 'Pagar
         Filtro = Filtro & " AND Conta = 'P'"
         If Not Vazio(Documento) Then 'Nº Documento
            Filtro = Filtro & " AND Documento = '" & Documento & "'"
         End If
      Else 'Receber
         Filtro = Filtro & " AND Conta = 'R'"
         If Not Vazio(Duplicata) Then 'Nº Duplicata
            Filtro = Filtro & " AND [Número da Duplicata] = '" & Duplicata & "'"
         End If
      End If
      If Not Vazio(Dt_Inicial) AND Not Vazio(Dt_Final) Then
         If Data = 0 Then 'Vencimento
            Filtro = Filtro & " AND PC.[Data de Vencimento] >= " & D(Dt_Inicial)
            Filtro = Filtro & " AND PC.[Data de Vencimento] <= " & D(Dt_Final)
         Else 'Emissão
            Filtro = Filtro & " AND E.[Data de Entrada] >= " & D(Dt_Inicial)
            Filtro = Filtro & " AND E.[Data de Entrada] <= " & D(Dt_Final)
         End If
      End If
      If Geral > 0 Then 'Geral
         Filtro = Filtro & " AND E.[Seqüência do Geral] " & Tipo_Geral & " " & Geral
      End If
   End If
End Function

