   Label(2).Enabled = Ok
   Ok = (Entrada = 0)
   Label(3).Enabled = Ok
   Ok = (Entrada = 0)
   Label(5).Enabled = Ok
   Ok = (Entrada = 0)
   Label(6).Enabled = Ok
