         Vazao = txtCampo(4).Value
         Raio_de_Alcance_metros = txtCampo(5).Value
         Area_Total_ha = txtCampo(6).Value
         Volume_Referencia_mm = txtCampo(7).Value
         Percentual_alcance_Molhado = txtCampo(8).Value
         Alcence_Raio_Molhado_m = txtCampo(9).Value
         Alcence_aspersor_final_ha = txtCampo(10).Value
         Fabricante_do_Aspersor = txtCampo(11).Value
         Sequencia_do_Bocal = txtCampo(0).Value
         Modelo_Aspersor = txtCampo(1).Value
      Else
         Bocal_do_Aspersor = IIf(IsNull(vgTb![Bocal do Aspersor]), 0, vgTb![Bocal do Aspersor])
         MCA = IIf(IsNull(vgTb!MCA), 0, vgTb!MCA)
         Vazao = IIf(IsNull(vgTb!Vazão), 0, vgTb!Vazão)
         Raio_de_Alcance_metros = IIf(IsNull(vgTb![Raio de Alcance metros]), 0, vgTb![Raio de Alcance metros])
         Area_Total_ha = IIf(IsNull(vgTb![Area Total ha]), 0, vgTb![Area Total ha])
         Volume_Referencia_mm = IIf(IsNull(vgTb![Volume Referencia mm]), 0, vgTb![Volume Referencia mm])
         Percentual_alcance_Molhado = IIf(IsNull(vgTb![Percentual alcance Molhado]), 0, vgTb![Percentual alcance Molhado])
         Alcence_Raio_Molhado_m = IIf(IsNull(vgTb![Alcence Raio Molhado m]), 0, vgTb![Alcence Raio Molhado m])
         Alcence_aspersor_final_ha = IIf(IsNull(vgTb![Alcence aspersor final ha]), 0, vgTb![Alcence aspersor final ha])
         Fabricante_do_Aspersor = IIf(IsNull(vgTb![Fabricante do Aspersor]), "", vgTb![Fabricante do Aspersor])
         Sequencia_do_Bocal = IIf(IsNull(vgTb![Sequencia do Bocal]), 0, vgTb![Sequencia do Bocal])
         Modelo_Aspersor = IIf(IsNull(vgTb![Modelo Aspersor]), "", vgTb![Modelo Aspersor])
