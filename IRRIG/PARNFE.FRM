'inicializa variaveis (apelidos) coms os campos correspondentes
Private Sub InicializaApelidos(vgComOQue As Integer)
   On Error Resume Next                           'prepara para possíveis erros
   If (vgTb.RecordCount > 0 And vgTb.Eof = False And vgTb.Bof = False) Or vgComOQue = COM_TEXTBOX Then
      If vgComOQue = COM_TEXTBOX Then
         Ambiente = Val(labopcPainel1.Caption)
         Diretorio_1_NFe_Homologacao = txtCampo(0).Value
         Diretorio_2_NFe_Homologacao = txtCampo(1).Value
         Diretorio_1_NFe_Producao = txtCampo(2).Value
         Diretorio_2_NFe_Producao = txtCampo(3).Value
         Diretorio_1_NFSe_Homologacao = txtCampo(4).Value
         Diretorio_2_NFSe_Homologacao = txtCampo(5).Value
         Diretorio_1_NFSe_Producao = txtCampo(6).Value
         Diretorio_2_NFSe_Producao = txtCampo(7).Value
         Certificado_Digital = txtCampo(8).Value
         Testemunha_1 = txtCampo(9).Value
         Testemunha_2 = txtCampo(11).Value
         CPF_Testemunha_1 = txtCampo(10).Value
         CPF_Testemunha_2 = txtCampo(12).Value
         Horario_de_Verao = chkCampo(0).Value
      Else
         Ambiente = IIf(IsNull(vgTb!Ambiente), 0, vgTb!Ambiente)
         Diretorio_1_NFe_Homologacao = IIf(IsNull(vgTb![Diretório 1 NFe Homologação]), "", vgTb![Diretório 1 NFe Homologação])
         Diretorio_2_NFe_Homologacao = IIf(IsNull(vgTb![Diretório 2 NFe Homologação]), "", vgTb![Diretório 2 NFe Homologação])
         Diretorio_1_NFe_Producao = IIf(IsNull(vgTb![Diretório 1 NFe Produção]), "", vgTb![Diretório 1 NFe Produção])
         Diretorio_2_NFe_Producao = IIf(IsNull(vgTb![Diretório 2 NFe Produção]), "", vgTb![Diretório 2 NFe Produção])
         Diretorio_1_NFSe_Homologacao = IIf(IsNull(vgTb![Diretório 1 NFSe Homologação]), "", vgTb![Diretório 1 NFSe Homologação])
         Diretorio_2_NFSe_Homologacao = IIf(IsNull(vgTb![Diretório 2 NFSe Homologação]), "", vgTb![Diretório 2 NFSe Homologação])
         Diretorio_1_NFSe_Producao = IIf(IsNull(vgTb![Diretório 1 NFSe Produção]), "", vgTb![Diretório 1 NFSe Produção])
         Diretorio_2_NFSe_Producao = IIf(IsNull(vgTb![Diretório 2 NFSe Produção]), "", vgTb![Diretório 2 NFSe Produção])
         Certificado_Digital = IIf(IsNull(vgTb![Certificado Digital]), "", vgTb![Certificado Digital])
         Testemunha_1 = IIf(IsNull(vgTb![Testemunha 1]), "", vgTb![Testemunha 1])
         Testemunha_2 = IIf(IsNull(vgTb![Testemunha 2]), "", vgTb![Testemunha 2])
         CPF_Testemunha_1 = IIf(IsNull(vgTb![CPF Testemunha 1]), "", vgTb![CPF Testemunha 1])
         CPF_Testemunha_2 = IIf(IsNull(vgTb![CPF Testemunha 2]), "", vgTb![CPF Testemunha 2])
         Horario_de_Verao = IIf(IsNull(vgTb![Horario de Verao]), 0, vgTb![Horario de Verao])
