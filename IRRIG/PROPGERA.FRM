Private Sub RepositionPropriedadesGeral()   
   TbAuxiliar "Geral", "[Seqüência do Geral] = " & Sequencia_do_Geral, GeralAux
   TbAuxiliar "Propriedades", "[Seqüência da Propriedade] = " & Sequencia_da_Propriedade, PropriedadeAux
   TbAuxiliar "Municípios", "[Seqüência do Município] = " & GeralAux![Seqüência Do Município], MunGeralAux
   TbAuxiliar "Municípios", "[Seqüência do Município] = " & PropriedadeAux![Seqüência Do Município], MunPropAux     
   MostraFormulas 
End Sub

Public Sub LigaDesligaBotoes
   Botao( 0 ).Enabled =    Botao( 0 ).Enabled And Permitido("Geral", ACAO_NAVEGANDO)
   Botao( 1 ).Enabled =    Botao( 1 ).Enabled And Permitido("Propriedades", ACAO_NAVEGANDO)
   Botao( 2 ).Enabled =    Botao( 2 ).Enabled And Permitido("Municípios", ACAO_NAVEGANDO)
   Botao( 3 ).Enabled =    Botao( 3 ).Enabled And Permitido("Municípios", ACAO_NAVEGANDO)
End Sub

Public Property Get txtCampoTab(Index As Integer) As FormataCampos   
   Set txtCampoTab = txtCampo(Index)
End Sub

'inicializa variaveis (apelidos) coms os campos correspondentes
Private Sub InicializaApelidos(vgComOQue As Integer)
   On Error Resume Next                           'prepara para possíveis erros
   If (vgTb.RecordCount > 0 And vgTb.Eof = False And vgTb.Bof = False) Or vgComOQue = COM_TEXTBOX Then
      If vgComOQue = COM_TEXTBOX Then
         Sequencia_do_Geral = txtCampo(1).Value
         Sequencia_da_Propriedade = txtCampo(2).Value
         Sequencia_da_Propriedade_Geral = Iif(vgSituacao = ACAO_INCLUINDO,0, vgTb![Seqüência da Propriedade Geral])
         Inativo = chkCampo(0).Value
      Else
         Sequencia_do_Geral = IIf(IsNull(vgTb![Seqüência do Geral]), 0, vgTb![Seqüência do Geral])
         Sequencia_da_Propriedade = IIf(IsNull(vgTb![Seqüência da Propriedade]), 0, vgTb![Seqüência da Propriedade])
         Sequencia_da_Propriedade_Geral = IIf(IsNull(vgTb![Seqüência da Propriedade Geral]), 0, vgTb![Seqüência da Propriedade Geral])
         Inativo = IIf(IsNull(vgTb!Inativo), 0, vgTb!Inativo)
      End If
