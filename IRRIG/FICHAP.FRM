Private Function SubTotal(SeqProd As Long, SeqBaixa As Long, QtdeAnterior As Double) As String
   Dim SQL As String, SubEstoque As New GRecordSet
   
   If SeqProd <> Produto Then PriVez = True
   Produto = SeqProd
   
   SQL = "If EXISTS(SELECT * FROM sysobjects WHERE id = object_id('AUXILIAR') AND sysstat & 0xf = 3) DROP TABLE AUXILIAR " & _
         "CREATE TABLE AUXILIAR (TotalParcial FLOAT) " & _
         "DECLARE @qteInicial FLOAT, @subEstoque FLOAT, @estoque FLOAT, @tipo INT, @Quantidade FLOAT, @Baixa INT " & _
         "SET @subEstoque = 0 " & _
         "DECLARE CUR_TMP CURSOR For " & _
         "SELECT [Tipo Do Movimento], Quantidade FROM [Baixa Do Estoque Contábil] WHERE [Seqüência Do Produto] = " & SeqProd & " AND [Data Do Movimento] < " & D(Dt_Inicial) & " ORDER BY [Data do Movimento], [Tipo do Movimento] " & _
         "OPEN CUR_TMP " & _
         "FETCH Next FROM CUR_TMP INTO @tipo, @Quantidade "
   SQL = SQL & "WHILE @@FETCH_STATUS = 0 " & _
               "BEGIN " & _
               "SET @estoque = Case @tipo when 0 then @Quantidade Else -@Quantidade end " & _
               "SET @SubEstoque = @SubEstoque + @estoque " & _
               "FETCH Next FROM CUR_TMP INTO @tipo, @Quantidade " & _
               "END " & _
               "CLOSE CUR_TMP " & _
               "DEALLOCATE CUR_TMP " & _
               "INSERT INTO AUXILIAR(TotalParcial) VALUES (@SubEstoque)"
   SQL = SQL & "DECLARE CUR_TMP CURSOR For " & _
               "SELECT [Seqüência da Baixa], [Tipo Do Movimento], Quantidade FROM [Baixa Do Estoque Contábil] WHERE [Seqüência da Baixa] = " & SeqBaixa & " AND [Data Do Movimento] >= " & D(Dt_Inicial) & " AND [Data do Movimento] <= " & D(Dt_Final) & " ORDER BY [Data do Movimento], [Tipo do Movimento] " & _
               "OPEN CUR_TMP " & _
               "FETCH Next FROM CUR_TMP INTO @Baixa, @tipo, @Quantidade " & _
               "While @@FETCH_STATUS = 0 " & _
               "Begin " & _
               "SET @estoque = Case @tipo when 0 then @Quantidade Else -@Quantidade end "
               If PriVez Then
                  SQL = SQL & "SET @SubEstoque = @SubEstoque + @estoque "
               Else
                  SQL = SQL & "SET @SubEstoque = " & QtdeAnterior & " + @estoque "
               End If
   SQL = SQL & "FETCH Next FROM CUR_TMP INTO @Baixa, @tipo, @Quantidade " & _
               "END " & _
               "CLOSE CUR_TMP " & _
               "DEALLOCATE CUR_TMP " & _
               "UPDATE AUXILIAR set TotalParcial = @SubEstoque"
        
   Set SubEstoque = vgDb.OpenRecordSet(SQL, , , , , , , , , , True)
   Set SubEstoque = vgDb.OpenRecordSet("SELECT TotalParcial FROM AUXILIAR")
   SubTotal = SubEstoque!TotalParcial
   PriVez = False

End Function

Private Function Filtro() As String
   Filtro = "1 = 1"
   Filtro = Filtro & " AND BE.[Seqüência do Produto] <> 0"
   Filtro = Filtro & " AND BE.[Seqüência da Baixa] <> 0"
   If Not Vazio(Dt_Inicial) Then
      Filtro = Filtro & " AND [Data do Movimento] >= " & D(Dt_Inicial)
   End If
   If Not Vazio(Dt_Final) Then
      Filtro = Filtro & " AND [Data do Movimento] <= " & D(Dt_Final)
   End If   
   If Not Vazio(Tipo_Movimento) Then
      If Tipo_Movimento = "Entrada" Then '0
         Filtro = Filtro & " AND [Tipo do Movimento] = 0"
      Else '1
         Filtro = Filtro & " AND [Tipo do Movimento] = 1"
      End If      
   End If
   If Not Vazio(Tipo_Produto) Then
      If Tipo_Produto = "P" Then '0
         Filtro = Filtro & " AND BE.[Tipo do Produto] = 0"
      Else '1
         Filtro = Filtro & " AND BE.[Tipo do Produto] = 0"
      End If
   End If
   If Produto_Inicial > 0 Then
      Filtro = Filtro & " AND BE.[Seqüência do Produto] = " & Produto_Inicial
   End If
   If Produto_Final > 0 Then
      Filtro = Filtro & " AND BE.[Seqüência do Produto] = " & Produto_Final
   End If

End Function

