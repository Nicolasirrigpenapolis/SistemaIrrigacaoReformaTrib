Private Function CustoMedio(Estoque_Atual As Double, Primeiro_Total As Currency, Total_Parcial As Currency, CustoAnterior As Currency, CustoInicial As Currency, EstoqueInicial As Double) As Currency
   On Error Resume Next
   If Produtos![Tipo Do Movimento] = 0 Then
      If CustoAnterior = 0 Then
         CustoMedio = (CustoInicial * EstoqueInicial + Produtos![Valor de Custo] * Produtos!Quantidade) / (EstoqueInicial + Produtos!Quantidade)
      Else
         CustoMedio = Round(CCur((IIf(PriVez, Primeiro_Total, Total_Parcial) + Movimento_de_Estoque![Quantidade Movimento] * Movimento_de_Estoque![Valor Unitário Movimento]) / Estoque_Atual), 4)
      End If
   Else
      CustoMedio = Round(CCur((IIf(PriVez, Primeiro_Total, Total_Parcial) - Movimento_de_Estoque![Quantidade Movimento] * Movimento_de_Estoque![Valor Unitário Movimento]) / Estoque_Atual), 4)
   End If
   Err.Clear
End Function

Private Function CalculaTotal(Tipo As Byte, Custo As Variant, QtdeInicial As Double, VrCusto As Currency, Qtde As Double, Prod As Long, Total_Parcial As Variant, Estoque As Double) As Currency
   If Tipo = 0 Then
      If (IIf(PriVez, SaldoInicial(Prod), Total_Parcial) = 0) Then
         CalculaTotal = Estoque * Custo
      Else
         CalculaTotal = IIf(PriVez, SaldoInicial(Prod), Total_Parcial) + Qtde * VrCusto
      End If
   Else
      If PriVez Then
         CalculaTotal = SaldoInicial(Prod) - CCur(Qtde) * CCur(Custo)
      Else
         CalculaTotal = CCur(Total_Parcial) - CCur(Qtde) * CCur(Custo)
      End If
   End If

End Function

'prepara o intervalo de páginas a ser impresso
Public Property Let vgIntervalPag(ByVal NovoValor As String)
   Dim x As String, z As String, i As Integer, j As Integer, k As Integer
   ReDim vgPagRef(0) As Integer
   vgIntPag$ = NovoValor$
   x$ = Substitui(vgIntPag$, ";", ",", UM_A_UM)
   j = 0
   Do While Len(x$) > 0
      z$ = Parse(x$, ",")
      i = InStr(z$, "-")
      If i > 0 Then
         k = Val(Mid(z$, i + 1))
         i = Val(Left(z$, i - 1))
      Else
         i = Val(z$)
         k = Val(z$)
      End If
      For i = i To k Step ((i > k) ^ (i > k))
         j = j + 1
         ReDim Preserve vgPagRef(j) As Integer
         vgPagRef(j) = i
      Next
   Loop
End Property

