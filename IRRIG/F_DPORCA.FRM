         Loop
      End If
   End With
      
   vgDb.CommitTrans
   
   If Me.Caption = "Duplica Orçamento" Then
      MsgBox "Orçamento Duplicado com Sucesso!!!", vbInformation + vbOKOnly, vaTitulo
      Unload Me
      
      'Atualizando Formulario
      If frmOrcament.vgSituacao = 0 Then 'Se nw esta aberto o Orçamento, entao vamos abrir ora pois
         Load frmOrcament
         frmOrcament.Show
         frmOrcament.SetFocus
         MoveRegistro mdiIRRIG.ActiveForm, REG_ULTIMO  'movimenta para o último registro
      Else
         frmOrcament.SetFocus
         mdiIRRIG.CancelaAlteracoes
         InicializaFiltro frmOrcament                  'Atualiza o Form
         MoveRegistro mdiIRRIG.ActiveForm, REG_ULTIMO  'movimenta para o último registro
      End If
      
   Else 'Proforma
      MsgBox "Fatura Proforma Duplicado com Sucesso!!!", vbInformation + vbOKOnly, vaTitulo
      Unload Me
      
      If FProformaAberto Then
         FProforma.SetFocus
         mdiIRRIG.CancelaAlteracoes
         InicializaFiltro FProforma                    'Atualiza o Form
         MoveRegistro mdiIRRIG.ActiveForm, REG_ULTIMO  'movimenta para o último registro
      Else
         FProforma.Caption = "Fatura Proforma"
         FProforma.vgFiltroInicial = "[Seqüência do Orçamento] > 0 AND [Fatura Proforma] = 1"
         InicializaFiltro FProforma
         FProforma.Reposition
         FProforma.Show
         MoveRegistro mdiIRRIG.ActiveForm, REG_ULTIMO  'movimenta para o último registro
         FProformaAberto = True
      End If
   End If
      
   Screen.MousePointer = vbDefault 'Ampulheta
   
DeuErro:
   If Err Then
      MsgBox Err.Description, vbCritical + vbOKOnly, vaTitulo
      vgDb.RollBackTrans
   End If
   
End Sub

Private Sub VerificaFormulario()
   On Error Resume Next
   
   If Me.Caption = "Duplica F. Proforma" Then
      FProformaDAberto = False
   End If

End Sub

