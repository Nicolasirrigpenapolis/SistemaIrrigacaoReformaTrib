Private Sub SuperQuery()
   Dim SQL As String
   
   On Error GoTo DeuErro
   
   SQL = "SELECT [Seqüência da Manutenção], [Seqüência da Entrada], [Seqüência Do Estoque], [Seqüência do Pedido], [Seqüência da Baixa], " & _
         "[Data de Entrada], [Seqüência Do Geral], [Razão Social], Documento, [Número da Duplicata], [Tipo da Conta], [Seqüência da Cobrança], " & _
         "Descrição, Parcela, [Data de Vencimento], [Valor da Parcela], [Valor Do Juros], [Valor Do Desconto], [Valor Restante], [Valor Total], " & _
         "[Data da Baixa], [Valor Pago], [Cheque Impresso], Tp " & _
         "FROM("
   
   SQL = SQL & "SELECT [Seqüência da Manutenção], [Seqüência da Origem] [Seqüência da Entrada], [Seqüência Do Estoque], [Seqüência do Pedido], [Seqüência da Baixa], " & _
               "[Data de Entrada], Geral.[Seqüência Do Geral], Geral.[Razão Social], Documento, [Número da Duplicata], [Tipo da Conta], " & _
               "[Tipo de Cobrança].[Seqüência da Cobrança], Descrição, Parcela, [Data de Vencimento], [Valor da Parcela], [Valor Do Juros], [Valor Do Desconto], " & _
               "[Valor Restante], [Valor Total], [Data da Baixa], [Valor Pago], [Cheque Impresso], Conta Tp " & _
               "FROM [Manutenção Contas] LEFT JOIN Geral ON [Manutenção Contas].[Seqüência Do Geral] = Geral.[Seqüência Do Geral] " & _
               "LEFT JOIN [Tipo de Cobrança] ON [Manutenção Contas].[Seqüência da Cobrança] = [Tipo de Cobrança].[Seqüência da Cobrança]) A " & _
               "WHERE " & Filtro() & _
               " Group By [Seqüência da Manutenção], [Seqüência da Entrada], [Seqüência Do Estoque], [Seqüência do Pedido], [Seqüência da Baixa], " & _
               "[Data de Entrada], [Seqüência Do Geral], [Razão Social], Documento, [Número da Duplicata], [Tipo da Conta], " & _
               "[Seqüência da Cobrança], Descrição, Parcela, [Data de Vencimento], [Valor da Parcela], [Valor Do Juros], [Valor Do Desconto], " & _
               "[Valor Restante], [Valor Total], [Data da Baixa], [Valor Pago], [Cheque Impresso], Tp"
   vgNovaQuery$ = SQL
   
DeuErro:
   If Err.Number <> 0 Then
      MsgBox Err.Description, vbCritical + vbOKOnly, vaTitulo
   End If

End Sub
   
Private Function Filtro() As String
      
   On Error Resume Next
      
   Filtro = "[Seqüência da Manutenção] > 0"
   If Tipo = 0 Then 'Pagar
      Filtro = Filtro & " AND Tp = 'P'"
   Else 'Receber
      Filtro = Filtro & " AND Tp = 'R'"
   End If
   If Entrada = 0 And Estoque = 0 And Baixa = 0 And Pedido = 0 Then
      If Data = 0 Then 'Vencimento
         If Not Vazio(Dt_Inicial) Then 'Data Inicial
            Filtro = Filtro & " AND [Data de Vencimento] >= " & D(Dt_Inicial)     'Vencimento
         End If
         If Not Vazio(Dt_Final) Then 'Data Final
            Filtro = Filtro & " AND [Data de Vencimento] <= " & D(Dt_Final)       'Vencimento
         End If
      ElseIf Data = 1 Then 'Pagamento
         If Not Vazio(Dt_Inicial) Then 'Data Inicial
            Filtro = Filtro & " AND [Data da Baixa] >= " & D(Dt_Inicial)     'Pagamento
         End If
         If Not Vazio(Dt_Final) Then 'Data Final
            Filtro = Filtro & " AND [Data da Baixa] <= " & D(Dt_Final)       'Pagamento
         End If
      Else 'Emissão
         If Not Vazio(Dt_Inicial) Then 'Data Inicial
            Filtro = Filtro & " AND [Data de Entrada] >= " & D(Dt_Inicial)     'Emissão
         End If
         If Not Vazio(Dt_Final) Then 'Data Final
            Filtro = Filtro & " AND [Data de Entrada] <= " & D(Dt_Final)       'Emissão
         End If
      End If
      If Geral > 0 Then 'Geral
         Filtro = Filtro & " AND [Seqüência do Geral] " & Tipo_Geral & " " & Geral
      End If
      If Duplicata > 0 Then 'Duplicata
         Filtro = Filtro & " AND [Número da Duplicata] = " & Duplicata
      End If      
      If Not Vazio(Documento) Then 'Documento
         Filtro = Filtro & " AND [Documento] Like '%" & Documento & "%'"
      End If
   Else
      If Entrada > 0 Then
         Filtro = Filtro & " AND [Seqüência da Manutenção] = " & Entrada
      End If
      If Estoque > 0 Then
         Filtro = Filtro & " AND [Seqüência da Manutenção] = " & Estoque
      End If
      If Baixa > 0 Then
         Filtro = Filtro & " AND [Seqüência da Manutenção] = " & Baixa
      End If
      If Pedido > 0 Then
         Filtro = Filtro & " AND [Seqüência da Manutenção] = " & Pedido
      End If
   End If
End Function

