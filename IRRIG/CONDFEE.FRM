   vgIdentTab = ""
   vgPriVez = True
   vgTipo = TP_COMUM
   vgCaracteristica = F_COMUM
   vgTemFiltro = False
   vgSituacao = ACAO_NAVEGANDO
   Set picBox(0).Picture = LoadPicture(LoadGasPicture(21525))
   Set botOk.Picture = LoadPicture(LoadGasPicture(21526))
   Set botCancela.Picture = LoadPicture(LoadGasPicture(21527))
   Set lblAjuste = Label(1)
   Set txtChave = txtCampo(0)
   DefineControles
   vgTooltips.Create
   Label(0).Caption = LoadGasString(30025)
   vgTooltips.AddTool botOk, 0, LoadGasString(30026)
   botOk.Caption = LoadGasString(30027)
   vgTooltips.AddTool botCancela, 0, LoadGasString(30028)
   botCancela.Caption = LoadGasString(30029)
   Label(1).Caption = LoadGasString(30030)
   Label(2).Caption = LoadGasString(30031)
   AjustaTamanho Me
   Executar INICIALIZACOES
   InicializaApelidos COM_TEXTBOX
   ExecutaVisivel
   Screen.MousePointer = vbDefault
   vgPriVez = False
   Exit Sub

DeuErro:
   CErr.NumErro = Err
   CErr.FunctionName = "Form_Load"
   CErr.Origem = CStr(vgFormID) + " - " + Me.Caption
   CErr.Show
