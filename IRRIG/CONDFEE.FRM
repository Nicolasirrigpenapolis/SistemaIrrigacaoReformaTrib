Public Sub ConsultaNotas()
   Dim siglaWS As String, tipoAmbiente As Long, nomeCertificado As String, versao As String
   Dim msgDados As String, msgRetWS As String, cStat As Long, msgResultado As String
   Dim CNPJ As String, cUF As String, ultNSURecebido As String, verAplic As String
   Dim dhResp As String, ultNSU As String, maxNSU As String, qtdeDocto As Long, txtNFe As String
   Dim proxy As String, usuario As String, senha As String, licenca As String
   Dim objNFeUtil As Object, Arq As long
     
   'Bora Começar
   
   Set objNFeUtil = CreateObject("NFe_Util_2G.util")
   
   siglaWS = "AN" ' sigla do WS: AN para Ambiente Nacional ou RS
   tipoAmbiente = IIf(Parametros_da_NFe!Ambiente = 0, 1, 2)  ' Ambiente: 1-Produção e 2-Homologação
   nomeCertificado = Parametros_da_NFe![Certificado Digital] ' campo assunto do certificado digital
   'licenca = "22997ab769e9dade323e6ff97dfb99c474210c66b2ae3930ffcc6d85e8b50399533b9762aa7a61fdccb6cba94e4d650ad00012c782e74b9c7e2f380e18f1517e"
   licenca = "23433296a6c7b3e6e602aa8c71ded18804ec302656cfa8590ff7b35454b14834f4f5aed7ed5b4e4986a6a2d2a4b1abad6758927b36d70c5a1d4588597eaf0de2"  
   versao = "4.00" ' versão= 1.00
   CNPJ = "02781892000130" ' CNPJ do destinatário da NF-e
   cUF = "35"                                 
   ultNSURecebido = txtChave.Text ' Ultimo nsu consultado
   
   'Devemos fazer um laço pois a Sefaz só pesquisa 50000 registros por vez
   'Do
      'ultNSU = ultNSUConsultado
      txtNFe = objNFeUtil.distNSU(siglaWS, tipoAmbiente, nomeCertificado, versao, msgDados, msgRetWS, CStat, msgResultado, CNPJ, cUF, ultNSURecebido, verAplic, dhResp, ultNSU, maxNSU, qtdeDocto, proxy, usuario, senha, licenca)
   'Loop While indCont = 1
   
   If CStat = 138 Then 'Sucesso
      MsgBox "Notas Consultada com Sucesso", vbInformation, "Sucesso"
      
      'Salvando o xml...
      Arq = FreeFile
      If Parametros_da_NFe!ambiente = 0 Then 'Produção
         If Dir(Parametros_da_NFe![Diretório 2 NFe Produção] & "CONSULTANFE.txt", vbArchive) <> "" Then Kill Parametros_da_NFe![Diretório 2 NFe Produção] & "CONSULTANFE.xml"
         Open Parametros_da_NFe![Diretório 1 NFe Produção] & "CONSULTANFE.txt" For Append As #Arq
            Print #Arq, txtNFe
         Close #Arq
      Else 'Homologação
         If Dir(Parametros_da_NFe![Diretório 2 NFe Homologação] & "CONSULTANFE.txt", vbArchive) <> "" Then Kill Parametros_da_NFe![Diretório 2 NFe Homologação] & "CONSULTANFE.xml"
         Open Parametros_da_NFe![Diretório 1 NFe Homologação] & "CONSULTANFE.txt" For Append As #Arq
            Print #Arq, txtNFe
         Close #Arq
      End If   
      
      'LeXML 'Vamos Ler o XML e Incluir os Dados no Banco   
     
   Else 'Ops deu Erro
      MsgBox msgResultado & vbCrLf & vbCrLf & txtNFe, vbInformation, "Resultado"
   End If
   
   Set objNFeUtil = Nothing
   
End Sub

'inicializa variaveis (apelidos) coms os campos correspondentes
Private Sub InicializaApelidos(vgComOQue As Integer)
   On Error Resume Next                           'prepara para possíveis erros
   Chave = txtCampo(0).Value
   If Err Then Err.Clear                          'se deu algum erro, vamos resetá-lo
End Sub

'executa processos/validacoes nos campos do formuário
Public Function Executar(vgOq As String) As String
   Dim vgMsg As String, vgOk As Integer, vgNVez As Integer
   On Error Goto DeuErro                          'fica na espera de um erro...
   vgMsg$ = ""                                    'retorna uma msg dizendo o motivo
   vgOk = True                                    'se a validação esta OK
   vgNVez = 0                                     'porque não fez o processo/validacoes
   If vgOq = INICIALIZACOES Then
   ElseIf vgOq = INI_APELIDOS Then
      InicializaApelidos COM_REGISTRO
      ExecutaVisivel
   End If
   Executar = vgMsg$                              'prepara saida da função
   Exit Function                                  'e cai fora...

DeuErro:
   Select Case Err                                'vamos verificar se deu algum erro
      Case 3197                                   'registro acabou de ser alterado por outro usuário
         Resume
      Case 3046, 3158, 3186, 3187, 3188, 3189, 3218, 3260
         vgNVez = vgNVez + 1                      'outro usuário acessando
         If vgNVez <= 10 Then                     'vamos tentar 10 vezes
            Delay 0.5                             'mais antes espera 1/2 segundo
            Resume                                'antes de tentar
         End If
   End Select
   Executar = Err.Source + "|" + Trim$(str$(Err)) + "|" + Error$ 'não teve jeito o erro não pode ser evitado...
End Function

