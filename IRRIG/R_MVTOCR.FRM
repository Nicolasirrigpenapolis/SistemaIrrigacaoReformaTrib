Private Function Filtro() As String
   Filtro = "1 = 1"
   Filtro = Filtro & " AND [Seqüência da Baixa] <> 0"
   Filtro = Filtro & " AND BE.[Seqüência do Conjunto] > 0"
   If Not Vazio(Mes) Then
      Filtro = Filtro & " AND [Data do Movimento] <= " & D(UltD(Mes))
   End If
   If Conjunto_Inicial > 0 Then
      Filtro = Filtro & " AND BE.[Seqüência do Conjunto] >= " & Conjunto_Inicial
   End If
   If Conjunto_Final > 0 Then
      Filtro = Filtro & " AND BE.[Seqüência do Conjunto] <= " & Conjunto_Final
   End If   
End Function

