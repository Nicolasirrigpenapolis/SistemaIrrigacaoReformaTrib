Private Sub MySQL()
   Dim sql As String

   On Error Resume Next
   
   sql = "SELECT [Seqüência Do Conjunto], Descrição, QtdeEntrada, VrEntrada, QtdeSaida, QuantidadeInicial, (QuantidadeInicial + QtdeEntrada - QtdeSaida) As QtdeFinal " & _
         "FROM(SELECT C.[Seqüência Do Conjunto], C.Descrição, ISNULL(SUM(Case [Tipo Do Movimento] WHEN 0 THEN Case WHEN [Data do Movimento] >= " & D(Filtro2) & " THEN Quantidade END END), 0) As QtdeEntrada, ISNULL(SUM(CASE WHEN [Data do Movimento] >= " & D(Filtro2) & " THEN (Quantidade * BE.[Valor de Custo]) ELSE 0 END), 0) As VrEntrada, ISNULL(SUM(Case [Tipo Do Movimento] WHEN 1 THEN Case WHEN [Data do Movimento] >= " & D(Filtro2) & " THEN Quantidade END END), 0) As QtdeSaida, " & _
         "ISNULL((SELECT SUM(A.Entrada - A.Saída) FROM( " & _
         "SELECT SUM(Quantidade) As Saída, 0 As Entrada FROM [Baixa Do Estoque Contábil] WHERE [Tipo Do Movimento] = 1 AND [Seqüência Do Conjunto] = C.[Seqüência Do Conjunto] AND [Data Do Movimento] < " & D(Filtro2()) & _
         " UNION ALL " & _
         "SELECT 0 As Saída, SUM(Quantidade) As Entrada FROM [Baixa Do Estoque Contábil] WHERE [Tipo Do Movimento] = 0 AND [Seqüência Do Conjunto] = C.[Seqüência Do Conjunto] AND [Data Do Movimento] < " & D(Filtro2()) & _
         ")A " & _
