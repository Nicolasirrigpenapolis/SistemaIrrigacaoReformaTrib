Private Sub LancaMPrima()
   Dim MPrima As New GRecordSet,Itens As New GRecordSet
   Dim Qtd As Double
   If Prod > 0 Then
   Set MPrima = vgDb.OpenRecordSet("SELECT * FROM [Matéria Prima] WHERE [Seqüência do Produto] = " & Prod)
   Set Itens = vgDb.OpenRecordSet("SELECT * FROM [Itens da Ordem de Produção]")
   
   If MPrima.RecordCount > 0 Then
   vgDb.BeginTrans
   
   'Materia Prima
   Do While Not MPrima.EOF
      With Itens 
         .AddNew
         !Sequencia = Sequencia_da_producao'SuperPegaSequencial("Ordem de Produção", "Sequencia da Produção")
         !Produto = MPrima![Seqüência da Matéria Prima]
         !Quant = MPrima![Quantidade de Matéria Prima]* Quantidade
         .Update
         .BookMark = .LastModified
      End With
     MPrima.MoveNext
   Loop
   vgDb.CommitTrans
  End If
 End If  
End Sub

