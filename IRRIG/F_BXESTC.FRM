Private Sub ProssegueExecucao()
   Dim Tb As New GRecordSet, i As Long
   Dim vgFGrid As New frmBrowse
   
   On Error Resume Next
   
   With vgFGrid
      Tag = "Baixa do Estoque Contábil"
            
      vgNovaQuery = "SELECT Case [Tipo do Movimento] When 0 Then ('Entrada') Else ('Saída') End [Tipo do Movimento], [Data Do Movimento], Documento, [Baixa do Estoque Contábil].[Seqüência Do Geral], Geral.[Razão Social], " & _
                    "[Baixa do Estoque Contábil].[Seqüência Do Produto], Produtos.Descrição [Descrição do Produto], [Baixa do Estoque Contábil].[Seqüência Do Conjunto], Conjuntos.Descrição [Descrição do Conjunto], " & _
                    "[Baixa do Estoque Contábil].[Seqüência da Despesa], Despesas.Descrição [Descrição da Despesa], " & _
