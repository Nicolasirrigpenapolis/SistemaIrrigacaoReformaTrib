Private Sub ManifestoNFe()
   Dim siglaWS As String, tipoAmbiente As Long, nomeCertificado As String, versao As String, chaveNFe As String
   Dim CNPJAutor As String, dhEvento As String, tpEvento As Long, xJust As String, proxy As String, usuario As String
   Dim senha As String, licenca As String, msgDados As String, msgRetWS As String, cStat As Long, msgResultado As String
   Dim nroProtocolo As String, dhProtocolo As String, procEvento As String, Arq As Long
   Dim objNFeUtil As Object
   
   Set objNFeUtil = CreateObject("NFe_Util_2G.util")

   siglaWS = "AN"
   tipoAmbiente = IIf(Parametros_da_NFe!ambiente = 0, 1, 2)
   nomeCertificado = Parametros_da_NFe![Certificado Digital]
   versao = "4.00"
   cStat = 0
   chaveNFe = Chave
   CNPJAutor = "02781892000130"
   dhEvento = Format$(Now, "yyyy-mm-dd HH:mm:ss")
   tpEvento = TipoEvento
   xJust = Justificativa
  ' licenca = "22997ab769e9dade323e6ff97dfb99c474210c66b2ae3930ffcc6d85e8b50399533b9762aa7a61fdccb6cba94e4d650ad00012c782e74b9c7e2f380e18f1517e"
  licenca = "23433296a6c7b3e6e602aa8c71ded18804ec302656cfa8590ff7b35454b14834f4f5aed7ed5b4e4986a6a2d2a4b1abad6758927b36d70c5a1d4588597eaf0de2"  
   ' 135 - Evento registrado e vinculado a NF-e
   ' 136 - Evento registrado, mas nao vinculado a NF-e
   procEvento = objNFeUtil.EnviaManDest(siglaWS, tipoAmbiente, nomeCertificado, versao, msgDados, msgRetWS, cStat, msgResultado, chaveNFe, CNPJAutor, dhEvento, tpEvento, xJust, nroProtocolo, dhProtocolo, proxy, usuario, senha, licenca)
   
   If cStat = 135 Then 'Sucesso
      MsgBox "NFe Manifestada com Sucesso", vbInformation, "Sucesso"
      
      'Salvando o xml...
      Arq = FreeFile
      If Parametros_da_NFe!ambiente = 0 Then 'Produção
         If Dir(Parametros_da_NFe![Diretório 2 NFe Produção] & Chave & "_MANIFESTO.xml", vbArchive) <> "" Then Kill Parametros_da_NFe![Diretório 2 NFe Produção] & Chave & "_MANIFESTO.xml"
         Open Parametros_da_NFe![Diretório 1 NFe Produção] & Chave & "_MANIFESTO.xml" For Append As #Arq
            Print #Arq, procEvento
         Close #Arq
      Else 'Homologação
         If Dir(Parametros_da_NFe![Diretório 2 NFe Homologação] & Chave & "_MANIFESTO.xml", vbArchive) <> "" Then Kill Parametros_da_NFe![Diretório 2 NFe Homologação] & Chave & "_MANIFESTO.xml"
         Open Parametros_da_NFe![Diretório 1 NFe Homologação] & Chave & "_MANIFESTO.xml" For Append As #Arq
            Print #Arq, procEvento
         Close #Arq
      End If
   
   Else 'Erro
   
      MsgBox msgResultado, vbExclamation, vaTitulo
   
   End If
   
   Set objNFeUtil = Nothing
   
End Sub

'inicializa variaveis (apelidos) coms os campos correspondentes
Private Sub InicializaApelidos(vgComOQue As Integer)
   On Error Resume Next                           'prepara para possíveis erros
   TipoEvento = Val(labopcPainel1.Caption)
   Justificativa = txtCampo(0).Value
   Chave = txtCampo(1).Value
   If Err Then Err.Clear                          'se deu algum erro, vamos resetá-lo
End Sub

'executa processos/validacoes nos campos do formuário
Public Function Executar(vgOq As String) As String
   Dim vgMsg As String, vgOk As Integer, vgNVez As Integer
   On Error Goto DeuErro                          'fica na espera de um erro...
   vgMsg$ = ""                                    'retorna uma msg dizendo o motivo
   vgOk = True                                    'se a validação esta OK
   vgNVez = 0                                     'porque não fez o processo/validacoes
   If vgOq = VALIDACOES Then
      InicializaApelidos COM_TEXTBOX
      If TipoEvento = 3 Then
         vgOk = (Not Vazio(Justificativa))
         vgMsg$ = "Justificativa não pode ser vazia"
         If Not vgOk Then txtCampo(0).SetFocus
      End If
      If vgOk Then vgMsg$ = ""
      DoEvents
   ElseIf vgOq = INICIALIZACOES Then
      InicializaApelidos COM_TEXTBOX
      On Error Resume Next
      opcPainel1(0).Value = True
      opcPainel1(0).CtPri.TabStop = False
   ElseIf vgOq = INI_APELIDOS Then
      InicializaApelidos COM_REGISTRO
      ExecutaVisivel
      ExecutaPreValidacao
   End If
   Executar = vgMsg$                              'prepara saida da função
   Exit Function                                  'e cai fora...

DeuErro:
   Select Case Err                                'vamos verificar se deu algum erro
      Case 3197                                   'registro acabou de ser alterado por outro usuário
         Resume
      Case 3046, 3158, 3186, 3187, 3188, 3189, 3218, 3260
         vgNVez = vgNVez + 1                      'outro usuário acessando
         If vgNVez <= 10 Then                     'vamos tentar 10 vezes
            Delay 0.5                             'mais antes espera 1/2 segundo
            Resume                                'antes de tentar
         End If
   End Select
   Executar = Err.Source + "|" + Trim$(str$(Err)) + "|" + Error$ 'não teve jeito o erro não pode ser evitado...
End Function

'executa a pré-validação dos campos
Private Sub ExecutaPreValidacao()
   Dim Ok As Boolean
   On Error Resume Next                           'prepara para possiveis erros
   Ok = (TipoEvento = 3)
   Label(2).Enabled = Ok
   Ok = (TipoEvento = 3)
   txtCampo(0).Enabled = (Ok And txtCampo(0).Editable)
   If Err Then Err.Clear                          'se houve erro, limpa...
End Sub

'coloca os campos visíveis segundo a condição
Private Sub ExecutaVisivel()
   On Error Resume Next                           'prepara para possiveis erros
   Label(1).Visible = (False)
   If Err Then Err.Clear                          'se houve erro, limpa...
End Sub

