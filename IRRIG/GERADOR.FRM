Private Function GeraSerie() As Long
Dim Tb As New GRecordSet

 Set Tb = vgDb.OpenRecordSet("Select Max([Serie do Gerador])Seq From [Serie Gerador]")
 
  If Tb.RecordCount > 0 Then
     GeraSerie = Tb!Seq + 1
  Else
     GeraSerie = 1
  End If
        
