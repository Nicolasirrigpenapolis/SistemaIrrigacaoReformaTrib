   Ok = ((((Not Vazio(Descri_do_Gerador)) And Not Vazio(Dt_de_Entrega)) And Not Vazio(Nro_do_Motor)) And Not Vazio(Nro_do_Gerador))
   chkCampo(0).Locked = Not (vgAlterar And chkCampo(0).Editable)
