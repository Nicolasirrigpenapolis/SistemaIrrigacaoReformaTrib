Private Sub ProssegueExecucao()
   Dim Tb As New GRecordSet, i As Long
   Dim vgFGrid As New frmBrowse
   
   On Error Resume Next
   
   With vgFGrid
      Tag = "Movimento da Conta Corrente"
            
      vgNovaQuery = "SELECT [Movimentação da Conta Corrente].[Seqüência da Agência], Agências.[Nome Do Banco], " & _
                    "[Conta Corrente da Agência].[Número da Conta Corrente], [Tipo de Movimento da CC], [Data Do Movimento], [Valor Total], " & _
                    "[Histórico da Conta Corrente].Descrição " & _
                    "FROM [Movimentação da Conta Corrente] INNER JOIN Agências ON [Movimentação da Conta Corrente].[Seqüência da Agência] = Agências.[Seqüência da Agência] " & _
                    "INNER JOIN [Conta Corrente da Agência] ON [Movimentação da Conta Corrente].[Seqüência da CC da Agência] = [Conta Corrente da Agência].[Seqüência da CC da Agência] AND " & _
                    "Agências.[Seqüência da Agência] = [Conta Corrente da Agência].[Seqüência da Agência] " & _
                    "LEFT JOIN [Histórico da Conta Corrente] ON [Movimentação da Conta Corrente].[Seqüência Do Histórico] = [Histórico da Conta Corrente].[Seqüência Do Histórico] " & _
                    "WHERE " & Filtro() & _
                    "GROUP BY [Seqüência da Movimentação CC], [Movimentação da Conta Corrente].[Seqüência da Agência], Agências.[Nome Do Banco], " & _
                    "[Conta Corrente da Agência].[Número da Conta Corrente], [Tipo de Movimento da CC], [Data Do Movimento], [Valor Total], " & _
                    "[Histórico da Conta Corrente].Descrição " & _
                    "ORDER BY [Seqüência da Movimentação CC]"
         
      Set Tb = vgDb.OpenRecordSet(vgNovaQuery)
           
      .vgTitConsulta = Tag
      With .grdBrowse         
         .ColumnWidth("Nome do Banco") = 5000            
         .ColumnWidth("Descrição") = 3700                        
         .ShowFilterBar = True
         .Stripes = Zebrar
      End With      
      .Show      
      DoEvents
    End With
End Sub

Private Function Filtro() As String
   On Error Resume Next
   
   Filtro = "[Seqüência da Movimentação CC] > 0"
   If Tipo = 0 Then 'Entrada
      Filtro = Filtro & " AND [Tipo de Movimento da CC] = 'Entrada'"
   Else 'Saída
      Filtro = Filtro & " AND [Tipo de Movimento da CC] = 'Saída'"
   End If
   If Not Vazio(Dt_Inicial) Then 'Data Inicial
      Filtro = Filtro & " AND [Data do Movimento] >= " & D(Dt_Inicial)          
   End If
   If Not Vazio(Dt_Final) Then 'Data Final
      Filtro = Filtro & " AND [Data do Movimento] <= " & D(Dt_Final)          
   End If
   If Banco > 0 Then 'Nome do Banco
      Filtro = Filtro & " AND [Movimentação da Conta Corrente].[Seqüência da Agência] " & Tipo_Banco & Banco
   End If
   If CC > 0 Then 'Conta corrente
      Filtro = Filtro & " AND [Movimentação da Conta Corrente].[Seqüência da CC da Agência] = " & CC
   End If
   If Historico > 0 Then 'Historico
      Filtro = Filtro & " AND [Movimentação da Conta Corrente].[Seqüência do Histórico] " & Tipo_Historico & Historico
   End If
   
End Function

