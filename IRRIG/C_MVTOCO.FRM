Private Sub SuperQuery()
   Dim SQL As String
   
   On Error GoTo DeuErro
   
   SQL = "SELECT Case [Tipo do Movimento] When 0 Then ('Entrada') Else ('Saída') End [Tipo do Movimento], [Data Do Movimento], Documento, [Baixa do Estoque Contábil].[Seqüência Do Geral], Geral.[Razão Social], " & _
         "[Baixa do Estoque Contábil].[Seqüência Do Produto], Produtos.Descrição [Descrição do Produto], [Baixa do Estoque Contábil].[Seqüência Do Conjunto], Conjuntos.Descrição [Descrição do Conjunto], " & _
         "[Valor Do PIS], [Valor Do Cofins], [Valor Do ICMS], [Valor da Substituição], [Valor Do Frete], [Valor Do IPI], Quantidade, " & _
         "[Valor Unitário], [Baixa do Estoque Contábil].[Valor de Custo], Quantidade * [Baixa do Estoque Contábil].[Valor de Custo] [Valor Total] " & _
         "FROM [Baixa do Estoque Contábil] INNER JOIN Geral ON [Baixa do Estoque Contábil].[Seqüência Do Geral] = Geral.[Seqüência Do Geral] " & _
         "LEFT JOIN Produtos ON [Baixa do Estoque Contábil].[Seqüência Do Produto] = Produtos.[Seqüência Do Produto] " & _
         "LEFT JOIN Conjuntos ON [Baixa do Estoque Contábil].[Seqüência Do Conjunto] = Conjuntos.[Seqüência Do Conjunto] " & _
         "WHERE " & Filtro() & _
         " GROUP BY [Tipo Do Movimento], [Data Do Movimento], Documento, [Baixa do Estoque Contábil].[Seqüência Do Geral], Geral.[Razão Social], " & _
         "[Baixa do Estoque Contábil].[Seqüência Do Produto], Produtos.Descrição, [Baixa do Estoque Contábil].[Seqüência Do Conjunto], Conjuntos.Descrição, " & _
         "[Valor Do PIS], [Valor Do Cofins], [Valor Do ICMS], [Valor da Substituição], [Valor Do Frete], [Valor Do IPI], Quantidade, " & _
         "[Valor Unitário], [Baixa do Estoque Contábil].[Valor de Custo], Quantidade * [Baixa do Estoque Contábil].[Valor de Custo]"
   vgNovaQuery$ = SQL
   
DeuErro:
   If Err.Number <> 0 Then
      MsgBox Err.Description, vbCritical + vbOKOnly, vaTitulo
   End If

End Sub

Private Function Filtro() As String
   On Error Resume Next
   
   Filtro = "[Seqüência da Baixa] > 0"
   If Tipo = 0 Then 'Entrada
      Filtro = Filtro & " AND [Tipo do Movimento] = 0"
   Else 'Saída
      Filtro = Filtro & " AND [Tipo do Movimento] = 1"
   End If
   If Not Vazio(Dt_Inicial) Then 'Data Inicial
      Filtro = Filtro & " AND [Data do Movimento] >= " & D(Dt_Inicial)          
   End If
   If Not Vazio(Dt_Final) Then 'Data Final
      Filtro = Filtro & " AND [Data do Movimento] <= " & D(Dt_Final)          
   End If
   If Not Vazio(Documento) Then 'Documento
      Filtro = Filtro & " AND Documento = '" & Documento & "'"
   End If
   If Geral > 0 Then 'Geral
      Filtro = Filtro & " AND [Baixa do Estoque Contábil].[Seqüência do Geral] " & Tipo_Geral & Geral
   End If
   If Tipo2 = 0 Then 'Produto
      If Produto > 0 Then
         Filtro = Filtro & " AND [Baixa do Estoque Contábil].[Seqüência do Produto] " & Tipo_Produto & Produto
      End If
   Else 'Conjunto
      If Conjunto > 0 Then
         Filtro = Filtro & " AND [Baixa do Estoque Contábil].[Seqüência do Conjunto] " & Tipo_Conjunto & Conjunto
      End If
   End If  
   
End Function

