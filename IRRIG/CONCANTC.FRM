Private Sub ConciliaConta()
   Dim Manutencao As New GRecordSet
   Dim Notas As New GRecordSet
   Dim PriVez As Boolean, NotasAux As String
   
   On Error GoTo DeuErro
   
   AjustaValores
      
   Set Manutencao = vgDb.OpenRecordSet("SELECT * FROM [Manutenção Contas] WHERE [Seqüência da Manutenção] = " & Sequencia_da_Manutencao)
   Set Notas = vgDb.OpenRecordSet("SELECT [Notas da Compra] FROM [Concilia Conta Antecipada] WHERE [Sequencia da Compra] = " & Sequencia_da_Compra)
   NotasAux = ""
   PriVez = True
   
   If Manutencao.RecordCount = 0 Then Exit Sub
      
    Do While Not Notas.EOF
       If PriVez Then
          NotasAux = Notas![Notas da Compra]
       Else
          NotasAux = NotasAux & "\" & Notas![Notas da Compra]
       End If
       PriVez = False
    Notas.MoveNext
    Loop
    
    If Notas.RecordCount = 0 Then
       NotasAux = Notas_da_Compra
    End If
    
    With Manutencao
       ![Notas da Compra] = NotasAux
        If Conciliado Then
           ![Conciliado] = 1
            VgDb.Execute"Update [Concilia Conta Antecipada] Set Conciliado = 1 Where [Sequencia da Compra] = " & Sequencia_da_Compra
        Else
            VgDb.Execute"Update [Manutenção Contas] Set Conciliado = 0 Where [Seqüência da Manutenção] = " & Sequencia_da_Manutencao 
            VgDb.Execute"Update [Concilia Conta Antecipada] Set Conciliado = 0 Where [Sequencia da Compra] = " & Sequencia_da_Compra      
        End If   
       .Update
       .BookMark = .LastModified
    End With
      
DeuErro:
   If Err.Number <> 0 Then
      MsgBox Err.Description, vbCritical + vbOKOnly, vaTitulo
      vgDb.RollBackTrans
   End If
   
End Sub


