         Sequencia_da_Compra = txtCampo(4).Value
         Data_da_Conciliacao = txtCampo(10).Value
         Notas_da_Compra = txtCampo(11).Value
         Conciliado = chkCampo(0).Value
      Else
         Sequencia_da_Conciliacao = IIf(IsNull(vgTb![Sequencia da Conciliação]), 0, vgTb![Sequencia da Conciliação])
         Sequencia_da_Manutencao = IIf(IsNull(vgTb![Seqüência da Manutenção]), 0, vgTb![Seqüência da Manutenção])
         Sequencia_da_Compra = IIf(IsNull(vgTb![Sequencia da Compra]), 0, vgTb![Sequencia da Compra])
         Data_da_Conciliacao = vgTb![Data da Conciliação]
         Notas_da_Compra = IIf(IsNull(vgTb![Notas da Compra]), "", vgTb![Notas da Compra])
         Conciliado = IIf(IsNull(vgTb!Conciliado), 0, vgTb!Conciliado)
      End If
      Tem_Divirgencia = chkCampo(1).Value
      Documento = txtCampo(12).Value
      Tipo = txtCampo(13).Value
      Valor = txtCampo(14).Value
   End If
   If Err Then Err.Clear                          'se deu algum erro, vamos resetá-lo
