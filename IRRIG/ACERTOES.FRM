Private Function ProcessaProdutos(Tipo As String, Sequencia_do_Acerto As Long, Sequencia_do_Acerto_Produto As Long, Sequencia_do_Produto As Long, _
   Quantidade As Double) As Boolean
      
   On Error GoTo DeuErro
         
   BaixaProduto 0, Sequencia_do_Produto, Quantidade, Tipo
   
DeuErro:
   If Err.Number <> 0 Then
      MsgBox Err.Description, vbCritical + vbOKOnly, vaTitulo
   End If
End Function

