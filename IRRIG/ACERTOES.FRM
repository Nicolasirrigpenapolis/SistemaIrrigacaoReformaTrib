   With grdProdutos
      .ShowSumBar = True
      .ShowFilterBar = True
      .HideStatus = True
   End With   
   With grdConjuntos
      .ShowSumBar = True
      .ShowFilterBar = True
      .HideStatus = True      
   End With
   
End Sub

Private Sub ComandosProdutos(KeyAscii As Integer, Sequencia_do_Acerto As Long, Sequencia_do_Acerto_Produto As Long, Sequencia_do_Produto As Long, _
   Quantidade As Double)
   On Error GoTo DeuErro
   
   If KeyAscii = vbKeyF12 Then
      With grdProdutos
         Select Case .ColumnField(.Col)
            Case "Seqüência do Produto"
               seqRegistro = .ColumnValue(.Row + 1, .Col)
               frmProdutos.Show            
