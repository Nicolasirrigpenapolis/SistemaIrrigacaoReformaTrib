   Set botOk.PictureDisabled = LoadPicture(LoadGasPicture(20957))
   Set botCancela.Picture = LoadPicture(LoadGasPicture(20958))
   Set Botao(0).Picture = LoadPicture(LoadGasPicture(20959))
