Private Function AjustaValores(Sequencia_da_Agencia As Integer, Sequencia_da_CC_da_Agencia As Integer, Numero_da_Conta_Corrente As String, _
   Descricao As String, Valor_de_Saida As Double, Valor_de_Entrada As Double, _
   Valor_Atual As Double, Inativo As Boolean, Optional Oq As String = "") As Currency
   Dim VrEntrada As New GRecordSet, VrSaida As New GRecordSet, VrTotal As Currency

   On Error GoTo DeuErro
   
   Set VrEntrada = vgDb.OpenRecordSet("SELECT SUM([Valor Total]) Entrada FROM [Movimentação da Conta Corrente] WHERE [Tipo de Movimento da CC] = 'Entrada' AND [Seqüência da Agência] = " & Sequencia_da_Agencia & " AND [Seqüência da CC da Agência] = " & Sequencia_da_CC_da_Agencia)
   Set VrSaida = vgDb.OpenRecordSet("SELECT SUM(-[Valor Total]) Saida FROM [Movimentação da Conta Corrente] WHERE [Tipo de Movimento da CC] = 'Saída' AND [Seqüência da Agência] = " & Sequencia_da_Agencia & " AND [Seqüência da CC da Agência] = " & Sequencia_da_CC_da_Agencia)
   
   'Atualizando
   vgDb.BeginTrans
   vgDb.Execute "Update [Conta Corrente da Agência] Set [Valor de Entrada] = " & Substitui(VrEntrada!Entrada, ",", ".", SO_UM) & " WHERE [Seqüência da CC da Agência] = " & Sequencia_da_CC_da_Agencia
   vgDb.Execute "Update [Conta Corrente da Agência] Set [Valor de Saída] = " & Substitui(VrSaida!Saida, ",", ".", SO_UM) & " WHERE [Seqüência da CC da Agência] = " & Sequencia_da_CC_da_Agencia
   vgDb.Execute "Update [Conta Corrente da Agência] Set [Valor Atual] = " & Substitui(VrEntrada!Entrada + VrSaida!Saida, ",", ".", SO_UM) & " WHERE [Seqüência da CC da Agência] = " & Sequencia_da_CC_da_Agencia
   vgDb.CommitTrans
   
   If Oq = "Entrada" Then
      AjustaValores = VrEntrada!Entrada
   ElseIf Oq = "Saida" Then
      AjustaValores = VrSaida!Saida
   Else
      AjustaValores = VrEntrada!Entrada + VrSaida!Saida
   End If

DeuErro:
   If Err Then
      vgDb.RollBackTrans
   End If

End Function

