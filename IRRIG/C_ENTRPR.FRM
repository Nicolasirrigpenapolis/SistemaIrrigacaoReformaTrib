'evento - valida digitações e prossegue a execução
Private Sub botOk_Click()
   Dim k As String, vgFGrid As New frmBrowse
   If vgPriVez Then Exit Sub
   vgPriVez = True
   k$ = Executar(VALIDACOES)                      'faz validaçao dos campos
   If Len(k$) > 0 Then                            'EPA!... tem campo errado
      If Instr(k$, "|") > 0 Then
         Err.Source = Parse$(k$, "|")
         Err.Number = Val(Parse$(k$, "|"))        'deu erro
         Err.Description = Parse$(k$, "|")
         CErr.NumErro = Err.Number
         CErr.Show
      Else
         Beep                                     'manda aviso sonoro
         MsgBox k$, vbCritical, vgAtencao$        'mostra mensagem do erro
      End If
      Screen.MousePointer = vbDefault             'reseta o pointer do mouse
      vgPriVez = False
      Exit Sub                                    'e sai para continuar a ediçao ou inclusao
   End If                                         'OK. os campos estao certos
   Screen.MousePointer = vbHourglass              'mouse = ampulheta
   vgFGrid.vgTitConsulta = LoadGasString(32580)
   vgFGrid.vgFromFormID = 758
   vgNovaQuery$ = "SELECT  [Seqüência da Entrada],   [Data de Entrada],  Tipo,  Seqüência,  Descrição,  Quantidade,   [Nome do Funcionário]   FROM(  SELECT  [Entrada na Receita].[Seqüência da Entrada], " + _
                     "  [Data de Entrada],  'Produto' Tipo,  [Produtos da Entrada Receita].[Seqüência do Produto] Seqüência, " + _
                     " Produtos.Descrição,  Quantidade,   [Nome do Funcionário]   FROM  [Entrada na Receita] INNER JOIN [Produtos da Entrada Receita] ON [Entrada na Receita].[Seqüência da Entrada] = " + _
                     "[Produtos da Entrada Receita].[Seqüência da Entrada] INNER JOIN Produtos ON [Produtos da Entrada Receita].[Seqüência do Produto] = " + _
                     "Produtos.[Seqüência do Produto]  UNION ALL  SELECT  [Entrada na Receita].[Seqüência da Entrada],   [Data de Entrada],  'Conjunto' Tipo, " + _
                     " [Conjuntos da Entrada Receita].[Seqüência do Conjunto] Seqüência,  Conjuntos.Descrição,  Quantidade, " + _
                     "  [Nome do Funcionário]   FROM  [Entrada na Receita] INNER JOIN [Conjuntos da Entrada Receita] ON [Entrada na Receita].[Seqüência da Entrada] = " + _
                     "[Conjuntos da Entrada Receita].[Seqüência da Entrada] INNER JOIN Conjuntos ON [Conjuntos da Entrada Receita].[Seqüência do Conjunto] = " + _
                     "Conjuntos.[Seqüência do Conjunto]) A  WHERE " & Filtro() & "  GROUP BY  [Seqüência da Entrada],   [Data de Entrada], " + _
                     " Tipo,  Seqüência,  Descrição,  Quantidade,   [Nome do Funcionário]"
   vgFGrid.vgFiltroInicial = ""
   UnLoad Me
   vgFGrid.grdBrowse.Move 0, 0                    'descarrega este form
   vgFGrid.Show                                   'mostra na grade
   Screen.MousePointer = vbDefault                'cursor do mouse = default
   vgFGrid.grdBrowse.Stripes = Zebrar: vgFGrid.vgFiltroInicial = Filtro
   vgPriVez = False
End Sub

