'inicializações, validações e processos para o grid
Private Function ExecutaGrid(Index As Integer, ColumnValue() As Variant, ByVal vgOq As Integer, Optional ByVal vgItem As Long, Optional ByVal vgCol As Integer, Optional vgIsValid As Boolean, Optional ByRef vgColumn As Integer, Optional vgErrorMessage As String, Optional KeyCodeAscii As Integer, Optional Shift As Integer) As Variant
   Select Case Index
      Case  0 
         ExecutaGrid = ExecutaGrid0(ColumnValue(), vgOq, vgItem, vgCol, vgIsValid, vgColumn, vgErrorMessage, KeyCodeAscii, Shift)
   End Select
End Function

'inicializações, validações e processos do grid filho
Private Function ExecutaGrid0(ColumnValue() As Variant, ByVal vgOq As Integer, Optional ByVal vgItem As Long, Optional ByVal vgCol As Integer, Optional vgIsValid As Boolean, Optional ByRef vgColumn As Integer, Optional vgErrorMessage As String, Optional KeyCodeAscii As Integer, Optional Shift As Integer) As Variant
   Dim vgRetVal As Variant, vgRsError As GRecordSet, x as String, vgNVez As Integer
   Dim Seq_da_Baixa As Long, Usu_Alteracao As String, Dt_Modificacao As Variant
   Dim Manutencao As Double, Dta_Baixa As Variant, Juros As Double
   Dim Desconto As Double, Vr_Pago As Double, Tp_Carteira As String
   Dim Bx_do_Cliente As Variant, Quem_Pagou As String, Vr_do_Cliente As Double
   Dim Seq_da_Agencia As Integer, Seq_Acc_da_Agencia As Integer
   vgPriVez = True
   If vgOq = PREVALIDACOES Then
      vgRetVal = False
   Else
      vgRetVal = ""
   End If
   vgNVez = 0
   On Error GoTo DeuErro
   If vgOq = CONTEUDODACOLUNA Then
      If Grid(0).Status <> ACAO_NAVEGANDO And vgItem = Grid(0).SelectedItem Then
         GoSub IniApDaCol
      Else
         GoSub IniApDaTb
      End If
      On Error Resume Next
      If Err Then Err.Clear
   ElseIf vgOq = CONDICOES_ESPECIAIS Then
      If vgSituacao <> ACAO_INCLUINDO Then
         GoSub IniApDaTb
      On Error Resume Next
         Grid(0).AllowInsert = (False)
      On Error Resume Next
         Grid(0).AllowEdit = (False)
      On Error Resume Next
         Grid(0).AllowDelete = (False)
      End If
      vgRetVal = ""
   ElseIf vgOq = ABRE_TABELA_GRID Then
      On Error Resume Next
      vgRetVal = "SELECT * FROM [Alteracao Baixa Contas]"

      'definindo a expressão de ligação com o pai
      x$ = "[Seq do Spy] = " & Spy_Baixa_Contas![Seq do Spy]
      vgRetVal = InsereSQL(vgRetVal, EXP_WHERE, x$)

   ElseIf vgOq = DEFAULTDASCOLUNAS Then
      GoSub IniApDaCol
      vgRetVal = Null
      Select Case vgCol
         Case 3
            Dt_Modificacao = Date
            If Grid(0).Col = 3 Then
               vgRetVal = Dt_Modificacao
            End If
         Case 5
            Dta_Baixa = Date
            If Grid(0).Col = 5 Then
               vgRetVal = Dta_Baixa
            End If
         Case 10
            Bx_do_Cliente = Date
            If Grid(0).Col = 10 Then
               vgRetVal = Bx_do_Cliente
            End If
      End Select
   Else
   End If
   GoTo FimDaSub
   Exit Function

IniApDaCol:
   On Error Resume Next
   Seq_da_Baixa = ColumnValue(1)
   Usu_Alteracao = ColumnValue(2) & ""
   Dt_Modificacao = ColumnValue(3)
   Manutencao = ColumnValue(4)
   Dta_Baixa = ColumnValue(5)
   Juros = ColumnValue(6)
   Desconto = ColumnValue(7)
   Vr_Pago = ColumnValue(8)
   Tp_Carteira = ColumnValue(9) & ""
   Bx_do_Cliente = ColumnValue(10)
   Quem_Pagou = ColumnValue(11) & ""
   Vr_do_Cliente = ColumnValue(12)
   Seq_da_Agencia = ColumnValue(13)
   Seq_Acc_da_Agencia = ColumnValue(14)
   If Err Then Err.Clear
   On Error GoTo DeuErro
   Return

IniApDaTb:
   On Error Resume Next
   If Alteracao_Baixa_Contas.Eof = False And Alteracao_Baixa_Contas.BOF = False And Alteracao_Baixa_Contas.RecordCount > 0 Then
      Seq_da_Baixa = Alteracao_Baixa_Contas![Seq da Baixa]
      Usu_Alteracao = Alteracao_Baixa_Contas![Usu Alteracao]
      Dt_Modificacao = Alteracao_Baixa_Contas![Dt Modificacao]
      Manutencao = Alteracao_Baixa_Contas!Manutencao
      Dta_Baixa = Alteracao_Baixa_Contas![Dta Baixa]
      Juros = Alteracao_Baixa_Contas!Juros
      Desconto = Alteracao_Baixa_Contas!Desconto
      Vr_Pago = Alteracao_Baixa_Contas![Vr Pago]
      Tp_Carteira = Alteracao_Baixa_Contas![Tp Carteira]
      Bx_do_Cliente = Alteracao_Baixa_Contas![Bx do Cliente]
      Quem_Pagou = Alteracao_Baixa_Contas![Quem Pagou]
      Vr_do_Cliente = Alteracao_Baixa_Contas![Vr do Cliente]
      Seq_da_Agencia = Alteracao_Baixa_Contas![Seq da Agencia]
      Seq_Acc_da_Agencia = Alteracao_Baixa_Contas![Seq Acc da Agencia]
   End If
   If Err Then Err.Clear
   On Error GoTo DeuErro
   Return

DeuErro:
   If vgOq = CONTEUDODACOLUNA Or vgOq = DEFAULTDASCOLUNAS Or vgOq < 0 Then
      vgRetVal = Null
   Else
      vgErrorMessage$ = Err.Source + "|" + Trim$(Str$(Err)) + "-" + Error$
      vgIsValid = False
   End If
   If Not vgRsError Is Nothing Then
      vgRsError.CancelUpdate
      vgErrorMessage$ = vgRsError.Table & "=>" & vgErrorMessage$
      Set vgRsError = Nothing
   End If
   Resume ResumeErro

ResumeErro:
   On Error Resume Next

FimDaSub:
   ExecutaGrid0 = vgRetVal
   vgPriVez = False
End Function

'evento - pega o valor inicial das colunas do grid
Private Sub Grid_GetColumnDefaultValue(Index As Integer, ByVal vgCol As Integer, vgColumns() As Variant, ByRef vgDefaultValue As Variant)
   vgDefaultValue = ExecutaGrid(Index, vgColumns(), DEFAULTDASCOLUNAS, , vgCol)
End Sub

